{
  "private": true,
  "scripts": {
    "html-ghpages": "html-minifier --collapse-whitespace --remove-comments --input-dir /tmp/before/index.html --output /tmp/after/index.html",
    "scss-ghpages": "sass --no-source-map --style=compressed /tmp/before/scss/style.css:/tmp/after/css/style.css",
    "start": "npm run html-ghpages && npm run scss-ghpages"
  }
}
