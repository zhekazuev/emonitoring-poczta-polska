{
  "private": true,
  "scripts": {
    "html": "html-minifier --collapse-whitespace --remove-comments --input-dir src/static/index.html --output dist/index.html",
    "scss": "sass --no-source-map --style=compressed src/scss/style.css:dist/css/style.css",
    "start": "npm run html && npm run scss"
  }
}
